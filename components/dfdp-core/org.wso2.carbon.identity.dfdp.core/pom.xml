<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <groupId>org.wso2.carbon.identity</groupId>
    <artifactId>org.wso2.carbon.identity.dfdp.core</artifactId>
    <version>1.0.0-SNAPSHOT</version>
    <packaging>bundle</packaging>
    <name>WSO2 Carbon - DFDP Core</name>
    <repositories>
        <repository>
            <id>wso2-nexus</id>
            <name>WSO2 Public Nexus Repository</name>
            <url>https://maven.wso2.org/nexus/content/groups/wso2-public/</url>
        </repository>
    </repositories>
    <dependencies>
        <dependency>
            <groupId>org.wso2.carbon.identity.framework</groupId>
            <artifactId>org.wso2.carbon.identity.application.authentication.framework</artifactId>
            <version>5.19.0</version>
        </dependency>
        <dependency>
            <groupId>org.wso2.carbon.identity.framework</groupId>
            <artifactId>org.wso2.carbon.idp.mgt</artifactId>
            <version>5.19.0</version>
        </dependency>
        <dependency>
            <groupId>org.wso2.carbon.identity.framework</groupId>
            <artifactId>org.wso2.carbon.identity.core</artifactId>
            <version>5.19.0</version>
        </dependency>
        <dependency>
            <groupId>org.wso2.carbon</groupId>
            <artifactId>org.wso2.carbon.utils</artifactId>
            <version>4.6.2-m4</version>
        </dependency>
        <dependency>
            <groupId>org.apache.commons</groupId>
            <artifactId>commons-lang3</artifactId>
            <version>3.12.0</version>
        </dependency>
        <dependency>
            <groupId>org.apache.commons</groupId>
            <artifactId>commons-collections4</artifactId>
            <version>4.4</version>
        </dependency>
        <dependency>
            <groupId>commons-logging</groupId>
            <artifactId>commons-logging</artifactId>
            <version>1.2</version>
        </dependency>
        <dependency>
            <groupId>javax.servlet</groupId>
            <artifactId>javax.servlet-api</artifactId>
            <version>4.0.1</version>
            <scope>provided</scope>
        </dependency>
    </dependencies>
    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.felix</groupId>
                <artifactId>maven-bundle-plugin</artifactId>
                <version>5.1.2</version>
                <extensions>true</extensions>
                <configuration>
                    <instructions>
                        <Bundle-SymbolicName>${project.artifactId}</Bundle-SymbolicName>
                        <Export-Package>org.wso2.carbon.identity.dfdp.core.*</Export-Package>
                        <Import-Package>
                            org.wso2.carbon.identity.application.authentication.framework;version="[7.8.23,8.0.0)",
                            org.wso2.carbon.identity.application.authentication.framework.config.model;version="[7.8.23,8.0.0)",
                            org.wso2.carbon.identity.application.authentication.framework.context;version="[7.8.23,8.0.0)",
                            org.wso2.carbon.identity.application.authentication.framework.exception;version="[7.8.23,8.0.0)",
                            org.wso2.carbon.identity.application.authentication.framework.model;version="[7.8.23,8.0.0)",
                            org.wso2.carbon.identity.application.authentication.framework.handler.request.impl;version="[7.8.23,8.0.0)",
                            org.wso2.carbon.identity.application.authentication.framework.handler.step.impl;version="[7.8.23,8.0.0)",
                            org.wso2.carbon.identity.application.authentication.framework.handler.sequence.impl;version="[7.8.23,8.0.0)",
                            org.wso2.carbon.identity.application.authentication.framework.handler.claims.impl;version="[7.8.23,8.0.0)",
                            org.wso2.carbon.identity.application.authentication.framework.store;version="[7.8.23,8.0.0)",
                            org.wso2.carbon.identity.application.authentication.framework.util;version="[7.8.23,8.0.0)",
                            org.wso2.carbon.identity.application.common.model;version="[7.8.23,8.0.0)",
                            org.wso2.carbon.identity.application.common.util;version="[7.8.23,8.0.0)",
                            org.wso2.carbon.identity.core.bean.context;version="[7.8.23,8.0.0)",
                            org.wso2.carbon.identity.core.util;version="[7.8.23,8.0.0)",
                            org.wso2.carbon.identity.event;version="[7.8.23,8.0.0)",
                            org.wso2.carbon.identity.event.event;version="[7.8.23,8.0.0)",
                            org.wso2.carbon.identity.event.services;version="[7.8.23,8.0.0)",
                            org.wso2.carbon.identity.event.handler;version="[7.8.23,8.0.0)",
                            org.wso2.carbon.idp.mgt;version="[7.8.23,8.0.0)",
                            *
                        </Import-Package>
                    </instructions>
                </configuration>
            </plugin>
        </plugins>
    </build>
</project>
